{% extends "webpage.nunjucks" %}
{% set serverRoot = "../.." %}

{% block body %}
<div class="m-2 p-3">
  <div class="mb-2">
    {% block formTitle %}{% endblock %}
    <div class="text-right">
      <a href="{{ backUrl | default('..') }}">Go back</a>
    </div>
  </div>
  <form class="border border-primary rounded p-3" method="{{ formMethod | default('post') }}">
    {% block form %}{% endblock %}
  </form>
</div>
{% endblock %}